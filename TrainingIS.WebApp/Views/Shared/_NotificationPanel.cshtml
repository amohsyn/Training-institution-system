@if (TempData["notification"] != null)
{
    AlertMessage alerMessage = (AlertMessage) TempData["notification"];

  //  @Html.Raw(TempData["notification"])


<script language='javascript'>
    $(function () {
         swal( "@Html.Raw( alerMessage.notificationType.ToString().ToUpper())" , "@Html.Raw(HttpUtility.JavaScriptStringEncode(alerMessage.message))" ,"@Html.Raw(alerMessage.notificationType)" );
    });
</script>;

}

