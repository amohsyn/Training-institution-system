@using GApp.Core.Localization

@if (TempData["notification"] != null)
{
    AlertMessage alerMessage = (AlertMessage) TempData["notification"];

  //  @Html.Raw(TempData["notification"])


<script language='javascript'>
    $(function () {
         swal( "@Html.Raw(GAppEnumLocalization.GetLocalValue(typeof(NotificationType), alerMessage.notificationType.ToString()))" , "@Html.Raw(HttpUtility.JavaScriptStringEncode(alerMessage.message))" ,"@Html.Raw( alerMessage.notificationType.ToString())" );
    });
</script>;

}

